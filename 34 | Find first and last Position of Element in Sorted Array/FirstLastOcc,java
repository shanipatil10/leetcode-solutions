class Solution {
    public int[] searchRange(int[] nums, int target) {
        return new int[]{firstOccurence(nums,target),lastOccurence(nums,target)};
      } 



      public int firstOccurence(int[]nums,int target){
      int low=0;
      int high=nums.length-1;
      int mid;
      int ans=-1;
      while(low<=high){
        mid=(low+high)/2;
        if(target==nums[mid]){
            ans=mid;
            high=mid-1;
        }
        else if(target>nums[mid])
        low=mid+1;
        else
        high=mid-1;
       }
       return ans;
      }


      public int lastOccurence(int []nums,int target){
      int low=0;
      int high=nums.length-1;
      int mid;
      int ans=-1;
      while(low<=high){
        mid=(low+high)/2;
        if(target==nums[mid]){
            ans=mid;
            low=mid+1;
        }
        else if(target>nums[mid])
        low=mid+1;
        else
        high=mid-1;
      }
      return ans;
      }
    }
